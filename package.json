{
  "name": "chat",
  "version": "1.0.0",
  "description": "React Chat",
  "private": true,
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "main": "./src/js/index.js",
  "scripts": {
    "start": "node server.js",
    "start-watch": "./node_modules/.bin/webpack --watch",
    "wp-server":
      "./node_modules/.bin/webpack-dev-server --progress --colors --config ./webpack.config.js",
    "sass": "./node_modules/.bin/node-sass -w src/scss -o src/css",
    "dev":
      "concurrently --kill-others \"npm run start-watch\" \"npm run wp-server\" \"npm run sass\"",
    "prod": "NODE_ENV=production node_modules/.bin/webpack -p",
    "prune": "npm prune"
  },
  "keywords": ["Chat", "React", "nodejs"],
  "author": "Nikolay Ivanov, Jenny Pavlova",
  "license": "ISC",
  "dependencies": {
    "clean-webpack-plugin": "^0.1.17",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.3",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.6.1",
    "prettier-eslint": "^8.8.1",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-dev-server": "^2.11.2",
    "webpack-hot-middleware": "^2.21.1"
  },
  "bugs": {
    "url": "https://github.com/nikolaycviv/ReactChat/issues"
  },
  "homepage": "https://github.com/nikolaycviv/ReactChat#readme"
}
