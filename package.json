{
    "name": "chat",
    "version": "1.0.0",
    "description": "React Chat",
    "private": true,
    "engines": {
        "node": ">=6.5",
        "npm": ">=3.10"
    },
    "main": "./src/js/index.js",
    "scripts": {
        "start-watch": "./node_modules/.bin/webpack --watch",
        "wp-server": "./node_modules/.bin/webpack-dev-server --progress --colors --config ./webpack.config.js",
        "sass": "./node_modules/.bin/node-sass -w src/static/scss -o src/static/css",
        "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run wp-server\" \"npm run sass\"",
        "prod": "NODE_ENV=production node_modules/.bin/webpack -p",
        "prod test": "set NODE_ENV=production | node_modules/.bin/webpack -p",
        "test": "jest",
        "test-watch": "npm run test --watch --notify",
        "test-cover": "npm run test --coverage",
        "coverage": "npm run test-cover && opn coverage/lcov-report/index.html"
    },
    "proxy": "http://localhost:3001",
    "keywords": [
        "Chat",
        "React",
        "nodejs"
    ],
    "author": "Nikolay Ivanov, Jenny Pavlova",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.2",
        "clean-webpack-plugin": "^0.1.17",
        "cookie-parser": "^1.4.3",
        "express": "^4.16.2",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-router": "^4.2.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-jest": "^22.4.1",
        "babel-loader": "^7.1.3",
        "babel-plugin-react-html-attrs": "^2.0.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "concurrently": "^3.5.1",
        "css-loader": "^0.28.10",
        "eslint": "^4.18.1",
        "eslint-config-standard": "^10.2.1",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-node": "^5.2.1",
        "eslint-plugin-promise": "^3.6.0",
        "eslint-plugin-react": "^7.7.0",
        "eslint-plugin-standard": "^3.0.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "favicons-webpack-plugin": "0.0.7",
        "file-loader": "^1.1.10",
        "history": "^4.7.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^2.29.0",
        "http-server": "^0.11.1",
        "node-sass": "^4.6.1",
        "react-hot-loader": "^3.1.3",
        "sass-loader": "^6.0.6",
        "url-loader": "^0.6.2",
        "webpack": "^3.11.0",
        "webpack-dev-middleware": "^2.0.6",
        "webpack-dev-server": "^2.11.2",
        "webpack-hot-middleware": "^2.21.1",
        "webpack-manifest-plugin": "^1.3.2"
    },
    "bugs": {
        "url": "https://github.com/nikolaycviv/ReactChat/issues"
    },
    "homepage": "https://github.com/nikolaycviv/ReactChat#readme"
}
