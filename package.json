{
  "name": "chat",
  "version": "1.0.0",
  "description": "React Chat",
  "private": true,
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "main": "./src/index.js",
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"npm run nodemon\" \"npm run sass\"",
    "react-start": "react-scripts start",
