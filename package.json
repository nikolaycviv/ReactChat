{
  "name": "chat",
  "version": "1.0.0",
  "description": "React Chat",
  "main": "./dist/index.html",
  "scripts": {
    "start-watch": "./node_modules/.bin/webpack --watch",
    "wp-server": "./node_modules/.bin/webpack-dev-server --progress --colors --config ./webpack.config.js",
    "sass": "node-sass -w src/scss -o src/css",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run wp-server\" \"npm run sass\"",
    "prod": "./node_modules/.bin/webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaycviv/ReactChat.git"
  },
  "keywords": [
    "Chat",
    "React"
  ],
  "author": "Nikolay Ivanov, Jenny Pavlova",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.6.1",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "bugs": {
    "url": "https://github.com/nikolaycviv/ReactChat/issues"
  },
  "homepage": "https://github.com/nikolaycviv/ReactChat#readme"
}
