    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prune": "npm prune"
  },
  "keywords": [
    "Chat",
    "React",
    "nodejs"
  ],
  "author": "Nikolay Ivanov, Jenny Pavlova",
  "license": "ISC",
  "dependencies": {
